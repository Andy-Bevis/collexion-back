<div class="table table-hover mt-4">
<form method="post" action="{{ path('app_back_my_object_delete', {'id': my_object.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ my_object.id) }}">
    <button class=" btn btn-danger">Delete</button>
</form>
</div>
